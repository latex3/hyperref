       \@setfontsize\normalsize\@xpt\@xiipt
       \abovedisplayskip \w@ppp{5}{3}{}% 10+2-5
       \abovedisplayshortskip \w@zpp{3}{0}% unchanged
       \belowdisplayshortskip \w@ppp{3}{2}{2}% 6+3-3
