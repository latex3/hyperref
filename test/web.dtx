  \ifnum\Hy@secnum@part>\c@secnumdepth
    \phantomsection
  \fi
    \ifnum \c@secnumdepth >\m@ne
      \refstepcounter{part}%
      \addcontentsline{toc}{part}{\web@tocPartTitle{#1}}%
    \else
