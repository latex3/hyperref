        \addtolength{\@tempdimb}{-2\@tempdima}%
        \edef\fullscreenwidth{\the\@tempdimb}%
        \linewidth\fullscreenwidth
        \textwidth\fullscreenwidth
        \hfuzz\linewidth
        \setlength{\oddsidemargin}{\coverpagemargin-1in}%
        \setlength{\evensidemargin}{\oddsidemargin}%
        \marginparsep=0pt
        \marginparwidth=0pt
        \parindent0pt
    \fi\fi
